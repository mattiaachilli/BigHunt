<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane fx:id="pause" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.scenecontroller.PauseSceneControllerImpl">
   <top>
      <Label fx:id="title" text="PAUSE" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <VBox alignment="CENTER" prefHeight="800.0" prefWidth="600.0" spacing="30.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="resumeGameBtn" mnemonicParsing="false" onAction="#resumeGame" text="RESUME GAME" />
            <Button fx:id="goToMenuBtn" mnemonicParsing="false" onAction="#backToMenu" text="GO TO MENU" />
         </children>
      </VBox>
   </center>
</BorderPane>
